{
  "name": "youtube-video-generator-infrastructure",
  "version": "1.0.0",
  "description": "AWS CDK infrastructure for YouTube auto video generator",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy --all",
    "deploy:dev": "npm run build && cdk deploy --all --profile dev",
    "deploy:prod": "npm run build && cdk deploy --all --profile prod",
    "destroy": "cdk destroy --all",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "clean": "rm -rf cdk.out dist node_modules"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.87.0",
    "constructs": "^10.0.0"
  }
}
